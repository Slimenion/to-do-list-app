{
  "expo": {
    "name": "To do list",
    "slug": "to-do-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c8f2ef"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#c8f2ef"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "First my react native app",
    "githubUrl": "https://github.com/Slimenion/to-do-list-app"
  }
}
